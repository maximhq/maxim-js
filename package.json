{
	"name": "@maximai/maxim-js",
	"description": "Maxim AI JS SDK. Visit https://getmaxim.ai for more info.",
	"version": "6.20.0",
	"scripts": {
		"build": "npx tsc -p tsconfig.lib.json && npm run copy-assets && npm run clean-package",
		"copy-assets": "copyfiles \"README.md\" \"LICENSE\" dist",
		"clean-package": "node scripts/clean-package.js",
		"publish": "node publish.mjs maxim-js $npm_config_version",
		"lint": "eslint \"src/**/*.ts\"",
		"test": "jest",
		"clean": "rimraf dist",
		"prebuild": "npm run clean",
		"docs:generate": "typedoc",
		"docs:clean": "rimraf docs",
		"docs:rename": "node scripts/rename-files.js",
		"docs:fix-links": "node scripts/fix-links.js",
		"docs:post-process": "node scripts/post-docs.js",
		"docs:mintlify": "node scripts/generate-mintlify-nav.js",
		"docs:build": "npm run docs:clean && npm run docs:generate && npm run docs:rename && npm run docs:fix-links && npm run docs:post-process && npm run docs:mintlify"
	},
	"devDependencies": {
		"@ai-sdk/anthropic": "^2.0.9",
		"@ai-sdk/google": "^2.0.23",
		"@ai-sdk/openai": "^2.0.23",
		"@langchain/anthropic": "^0.3.21",
		"@langchain/community": "^0.3.46",
		"@langchain/core": "^0.3.0",
		"@langchain/langgraph": "^0.3.1",
		"@langchain/openai": "^0.5.12",
		"@openai/agents": "^0.1.9",
		"@types/jest": "^29.5.14",
		"@types/node": "22.15.30",
		"@typescript-eslint/eslint-plugin": "8.33.1",
		"@typescript-eslint/parser": "8.33.1",
		"ai": "^5.0.72",
		"ai-sdk-provider-v1": "npm:@ai-sdk/provider@^1.0.0",
		"ai-sdk-provider-v2": "npm:@ai-sdk/provider@^2.0.0",
		"copyfiles": "2.4.1",
		"dotenv": "^16.5.0",
		"eslint": "9.28.0",
		"jest": "29.7.0",
		"langchain": "^0.3.28",
		"openai": "^6.6.0",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.3.4",
		"ts-node": "^10.9.2",
		"tslib": "2.8.1",
		"typedoc": "^0.28.5",
		"typedoc-plugin-frontmatter": "^1.3.0",
		"typedoc-plugin-markdown": "^4.6.4",
		"typescript": "5.8.3",
		"uuid": "11.1.0",
		"zod": "^3.25.76"
	},
	"peerDependencies": {
		"@types/mime-types": "3.0.1",
		"expo-crypto": "^13.0.0",
		"mime-types": "3.0.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"ai",
		"gen-ai",
		"llm",
		"openai"
	],
	"author": {
		"name": "Maxim Engineering",
		"email": "eng@getmaxim.ai"
	},
	"type": "commonjs",
	"main": "index.js",
	"typings": "index.d.ts",
	"react-native": "index.react-native.js",
	"exports": {
		".": {
			"react-native": "./index.react-native.js",
			"types": "./index.d.ts",
			"default": "./index.js"
		},
		"./langchain": {
			"react-native": "./langchain.js",
			"types": "./langchain.d.ts",
			"default": "./langchain.js"
		},
		"./vercel-ai-sdk": {
			"react-native": "./vercel-ai-sdk.js",
			"types": "./vercel-ai-sdk.d.ts",
			"default": "./vercel-ai-sdk.js"
		},
		"./platform": {
			"react-native": "./src/lib/platform/reactNative.js",
			"types": "./src/lib/platform/adapter.d.ts",
			"default": "./src/lib/platform/index.js"
		},
		"./openai-agents": {
			"react-native": "./openai-agents.js",
			"types": "./openai-agents.d.ts",
			"default": "./openai-agents.js"
		}
	},
	"dependencies": {
		"axios": "^1.11.0",
		"axios-retry": "^4.5.0"
	},
	"peerDependenciesMeta": {
		"expo-crypto": {
			"optional": true
		}
	}
}
